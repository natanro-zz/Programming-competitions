#include <bits/stdc++.h>
using namespace std;

int calculaTempo(vector<int> tempo, vector<int> numDePizza, int tamMochila, int N){
	vector<vector<int>> tabulation(N+1,vector<int>(tamMochila+1));
	for(int i=0; i<=N; i++){
		for(int j=0; j<=tamMochila; j++){
			if(i==0 || j==0) tabulation[i][j] = 0;
			else if(numDePizza[i-1] > j) tabulation[i][j] = tabulation[i-1][j];
			else{
				tabulation[i][j] = max(tabulation[i-1][j] , tabulation[i-1][j-numDePizza[i-1]] +tempo[i-1]);
				//cout << "i: "<< i << " j: " << j << " " << tabulation[i][j]<<endl; 
			}
		}
	}
	return tabulation[N][tamMochila];
}

int main(){
	int N;
	while(cin>>N && N){
		int tamMochila;
		cin >> tamMochila;
		vector<int> tempo(N, 0), numDePizza(N, 0);
		for(int i=0; i<N; i++){
			int c, w;
			cin >> c >> w;
			tempo[i] = c;
			numDePizza[i] = w;
		}
		cout << calculaTempo(tempo, numDePizza, tamMochila, N) << " min."<<endl;
	}
	return 0;
}
